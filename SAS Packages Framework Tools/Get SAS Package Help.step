\":\"Enter package name\",\"placeholder\":\"\",\"regexp\":\"\",\"required\":true,\"type\":\"textfield\",\"visible\":\"\"},{\"id\":\"spf_help_keyword\",\"label\":\"(Optional) keyword to search\",\"placeholder\":\"\",\"required\":false,\"type\":\"textfield\",\"visible\":\"\"},{\"id\":\"spf_package_content_ds\",\"label\":\"Generate dataset of package contents in the WORK directory\",\"type\":\"checkbox\",\"visible\":\"\"},{\"id\":\"spf_help_output_type\",\"items\":[{\"value\":\"Results window\"},{\"value\":\"Log\"}],\"label\":\"Output location\",\"type\":\"radiogroup\",\"visible\":\"\"}],\"id\":\"page1\",\"label\":\"Main\",\"type\":\"page\"}],\"promptHierarchies\":[],\"showPageContentOnly\":true,\"syntaxversion\":\"1.3.0\",\"values\":{\"spf_help_keyword\":\"\",\"spf_help_output_type\":{\"value\":\"Results window\"},\"spf_package\":\"\",\"spf_package_content_ds\":false}}","templates":{"SAS":"/* Program based on SAS KB 51626: How to write the contents of the SAS log or other text output to an HTML, RTF, or PDF file \r\n   https://support.sas.com/kb/51/626.html\r\n*/\r\n%macro spf_package_help;\r\n    %local log_loc;\r\n\r\n    %if NOT %sysmacexist(helpPackage) %then %do;\r\n        %put ERROR: You must initialize the SAS Packages Framework before running the Package Help step.;\r\n        %abort;\r\n    %end;\r\n\r\n    /* Remove versions if user added them */\r\n    data _null_;\r\n        package = prxchange(\"s/[({\\[\\]=].*//\", -1, strip(\"&spf_package\"));\r\n        call symputx('spf_package', package, 'L');\r\n        call symputx('n_packages', countw(package, ' ') );\r\n    run;\r\n\r\n    %if &n_packages = 0 %then %do;\r\n        %put ERROR: You must include a package to get help.;\r\n        %abort;\r\n    %end;\r\n\r\n    %else %if &n_packages > 1 %then %do;\r\n        %put ERROR: &n_packages packages were specified but expected 1. Specify only one package when using the One package option.;\r\n        %abort;\r\n    %end;\r\n    \r\n    /* Get original log location and save an spf_package_help.log file to WORK */\r\n    %let log_loc      = %superq(SYSPRINTTOLOG);\r\n    %let spf_help_log = %sysfunc(getoption(work))/spf_package_help.log;\r\n    %let spf_help_output_type = %upcase(&spf_help_output_type);\r\n\r\n    %if &spf_help_output_type NE LOG %then %do;\r\n        proc printto log=\"&spf_help_log\" new; \r\n        run;\r\n    %end;\r\n\r\n    %helpPackage(&spf_package, &spf_help_keyword, packageContentDS=&spf_package_content_ds);\r\n\r\n    %if &spf_help_output_type NE LOG %then %do;\r\n\r\n        /* Reset log location */\r\n        %if &log_loc = %then %do;\r\n            proc printto; run;\r\n        %end;\r\n            %else %do;\r\n                proc printto file=\"&log_loc\"; run;\r\n            %end;\r\n\r\n        proc document name=spf_help(write);\r\n            import textfile=\"&spf_help_log\" to logfile;\r\n            replay;\r\n        quit;\r\n    %end;\r\n\r\n%mend;\r\n%spf_package_help;"}}
